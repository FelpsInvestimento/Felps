    <script>
        // Theme toggle functionality
        const themeToggle = document.getElementById('theme-toggle');
        const themeIcon = themeToggle.querySelector('i');
        
        themeToggle.addEventListener('click', () => {
            document.documentElement.classList.toggle('dark');
            if (document.documentElement.classList.contains('dark')) {
                themeIcon.classList.remove('fa-moon');
                themeIcon.classList.add('fa-sun');
            } else {
                themeIcon.classList.remove('fa-sun');
                themeIcon.classList.add('fa-moon');
            }
        });
        
        // Simulate real-time updates
        setInterval(() => {
            // Flash update indicator
            const apiStatus = document.getElementById('api-status');
            apiStatus.classList.add('pulse');
            setTimeout(() => {
                apiStatus.classList.remove('pulse');
            }, 2000);
        }, 10000);
        
        // Mode selection
        const modeCards = document.querySelectorAll('.mode-card');
        modeCards.forEach(card => {
            card.addEventListener('click', () => {
                modeCards.forEach(c => {
                    c.classList.remove('border-2', 'border-blue-500', 'glow');
                    c.classList.add('border-2', 'border-gray-700');
                    const status = c.querySelector('div:last-child');
                    status.classList.remove('bg-blue-900');
                    status.classList.add('bg-gray-700');
                    status.textContent = 'Inativo';
                });
                
                card.classList.remove('border-2', 'border-gray-700');
                card.classList.add('border-2', 'border-blue-500', 'glow');
                const status = card.querySelector('div:last-child');
                status.classList.remove('bg-gray-700');
                status.classList.add('bg-blue-900');
                status.textContent = 'Ativo';
            });
        });
        
        // Password toggle
        function togglePassword(fieldId) {
            const field = document.getElementById(fieldId);
            const icon = field.nextElementSibling.querySelector('i');
            if (field.type === 'password') {
                field.type = 'text';
                icon.classList.remove('fa-eye');
                icon.classList.add('fa-eye-slash');
            } else {
                field.type = 'password';
                icon.classList.remove('fa-eye-slash');
                icon.classList.add('fa-eye');
            }
        }
        
        // Terminal log functions
        const terminal = document.getElementById('terminal');
        
        function addLog(message, type = 'info') {
            const line = document.createElement('div');
            line.className = 'terminal-line';
            
            if (type === 'error') {
                line.classList.add('terminal-error');
            } else if (type === 'success') {
                line.classList.add('terminal-success');
            } else if (type === 'warning') {
                line.classList.add('terminal-warning');
            } else if (type === 'info') {
                line.classList.add('terminal-info');
            }
            
            const now = new Date();
            line.textContent = `[${now.toLocaleTimeString()}] ${message}`;
            terminal.appendChild(line);
            terminal.scrollTop = terminal.scrollHeight;
        }
        
        // API Connection Simulation with Progress
        const connectBtn = document.getElementById('connect-btn');
        const testConnectionBtn = document.getElementById('test-connection');
        const lastUpdate = document.getElementById('last-update');
        const connectionError = document.getElementById('connection-error');
        const apiStatusBtn = document.getElementById('api-status');
        const progressFill = document.getElementById('progress-fill');
        const progressText = document.getElementById('progress-text');
        const clearLogsBtn = document.getElementById('clear-logs');
        
        // Function to simulate API connection
        function connectToNovaDAX() {
            // Reset progress
            progressFill.style.width = '0%';
            progressText.textContent = '0%';
            
            // Show connecting state
            connectBtn.innerHTML = '<i class="fas fa-spinner fa-spin mr-2"></i> Conectando...';
            connectBtn.disabled = true;
            connectionError.textContent = '';
            
            addLog('Iniciando conex√£o com NovaDAX...', 'info');
            
            // Simulate connection process with progress
            let progress = 0;
            const interval = setInterval(() => {
                progress += 5;
                progressFill.style.width = `${progress}%`;
                progressText.textContent = `${progress}%`;
                
                if (progress === 20) {
                    addLog('Validando credenciais...', 'info');
                } else if (progress === 40) {
                    addLog('Conectando ao servidor...', 'info');
                } else if (progress === 60) {
                    addLog('Autenticando...', 'info');
                } else if (progress === 80) {
                    addLog('Verificando permiss√µes...', 'info');
                }
                
                if (progress >= 100) {
                    clearInterval(interval);
                    
                    // Simulate connection result
                    setTimeout(() => {
                        // Simulate connection failure
                        connectBtn.innerHTML = '<i class="fas fa-plug mr-2"></i> Conectar com NovaDAX';
                        connectBtn.disabled = false;
                        
                        // Update status
                        apiStatusBtn.className = 'flex items-center space-x-2 bg-red-600 px-4 py-2 rounded-lg';
                        apiStatusBtn.innerHTML = '<span class="status-indicator status-warning"></span><span>Erro de Conex√£o</span>';
                        
                        // Update last update time
                        const now = new Date();
                        lastUpdate.textContent = now.toLocaleString('pt-BR');
                        
                        // Show error message
                        connectionError.textContent = 'Erro: Chave API inv√°lida. Verifique suas credenciais.';
                        addLog('Erro: Chave API inv√°lida. Verifique suas credenciais.', 'error');
                        addLog('Falha na conex√£o com NovaDAX', 'error');
                        
                        // Add detailed error analysis
                        addLog('An√°lise de erro:', 'warning');
                        addLog('- Verifique se as chaves API est√£o corretas', 'warning');
                        addLog('- Confirme se a chave tem permiss√µes de leitura', 'warning');
                        addLog('- Verifique se a chave n√£o expirou', 'warning');
                        addLog('- Confirme se o IP est√° autorizado na NovaDAX', 'warning');
                    }, 500);
                }
            }, 100);
        }
        
        // Function to test connection
        function testConnection() {
            addLog('Testando conex√£o...', 'info');
            addLog('Erro: Chave API inv√°lida. Verifique suas credenciais.', 'error');
            addLog('An√°lise detalhada:', 'warning');
            addLog('1. Access Key: fb17caa1-00a5-45ad-800f-31dcff935376', 'info');
            addLog('2. Secret Key: IDqP2LKyivmnBprsRQ8qzLt6oQPQNWGo', 'info');
            addLog('3. Status: Chave inv√°lida ou sem permiss√µes', 'error');
        }
        
        connectBtn.addEventListener('click', connectToNovaDAX);
        testConnectionBtn.addEventListener('click', testConnection);
        
        clearLogsBtn.addEventListener('click', () => {
            terminal.innerHTML = '';
            addLog('Logs limpos', 'info');
        });
        
        // Add initial logs
        setTimeout(() => {
            addLog('Sistema pronto para conex√£o', 'info');
            addLog('Insira suas credenciais da NovaDAX e clique em "Conectar"', 'info');
            addLog('Para testes, clique em "Testar Conex√£o"', 'info');
        }, 1000);
    </script>
</body>
</html>
